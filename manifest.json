{
  "name": "HappyTab",
  "description": "A new page extension",

  "manifest_version": 3,
  "version": "1.2.0",

  "chrome_url_overrides" : {
    "newtab": "html/happytab.html"
  },

  "options_ui": {
	  "page": "html/options.html",
	  "open_in_tab": true
  },

  "icons": {
    "64": "assets/pineapple-64.png",
    "128": "assets/pineapple-128.png"
  },

  "permissions": ["bookmarks"]
}
